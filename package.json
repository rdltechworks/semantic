{
  "name": "semantic",
  "cloudflare": {
    "label": "Semantic",
    "products": [
      "Workers"
    ],
    "categories": [
      "Security"
    ],
    "publish": true
  },
  "dependencies": {
    "@babylonjs/core": "^8.20.0",
    "@loadable/component": "^5.16.7",
    "cobe": "0.6.3",
    "hono": "^4.8.3",
    "partyserver": "0.0.66",
    "partysocket": "1.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.21",
    "esbuild": "0.25.5",
    "esbuild-plugin-copy": "^2.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "wrangler": "4.16.1"
  },
  "scripts": {
    "cf-typegen": "wrangler types ./src/server/worker-configuration.d.ts",
    "check": "tsc --project src/client && tsc --project src/server && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "start": "npm run build && npx wrangler dev",
    "build": "npx esbuild src/client/index.tsx --bundle --outdir=public/dist --splitting --sourcemap --format=esm",
    "test": "jest",
    "predeploy": "wrangler d1 migrations apply DB --remote",
    "seedLocalD1": "wrangler d1 migrations apply DB --local"
